        <activity
                android:name=".MainActivity"
                android:theme="@style/AppSplash"
                android:screenOrientation="portrait"
                android:launchMode="singleTask"
                >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!-- 登录 -->
        <activity
                android:name=".login.LoginActivity"
                android:label="@string/login"
                android:screenOrientation="portrait"/>
        <!-- 注册 -->
        <activity
                android:name=".login.RegisterActivity"
                android:label="@string/registerTitle"
                android:screenOrientation="portrait"/>
        <!-- PhotoView -->
        <activity
                android:name=".activity.PhotoViewActivity"
                android:screenOrientation="portrait"/>
        <!-- 工具 activity -->
        <activity
                android:name=".activity.ToolsActivity"
                android:label="@string/tools"
                android:screenOrientation="portrait"/>
        <!-- 手機相關信息 -->
        <activity
                android:name=".activity.PhoneUtilsActivity"
                android:label="@string/phone_system"
                android:screenOrientation="portrait"/>
        <!-- SelectorButton -->
        <activity
                android:name=".activity.SelectorButtonActivity"
                android:label="@string/selector_button"
                android:screenOrientation="portrait"/>
        <!-- 美图欣赏 -->
        <activity
                android:name=".activity.BelleActivity"
                android:label="@string/pretty_picture"
                android:screenOrientation="portrait"/>
        <!-- 美图欣赏 图片详情页 -->
        <activity
                android:name=".activity.PictureDetailActivity"
                android:screenOrientation="portrait"/>
        <!-- AgentWeb -->
        <activity
                android:name=".web.AgentWebActivity"
                android:screenOrientation="portrait"/>
        <!-- 指紋相关工具类 -->
        <activity
                android:name=".activity.FingerprintMainActivity"
                android:label="@string/fingerprint"
                android:screenOrientation="portrait"/>
        <!-- 修改个人信息 activity -->
        <activity
                android:name=".modify.ModifyInfoActivity"
                android:label="@string/myData"
                android:screenOrientation="portrait"/>
        <!-- 关于我们 -->
        <activity
                android:name=".about.AboutActivity"
                android:label="@string/about_us"
                android:screenOrientation="portrait"/>




        <!-- 相机Camera2 -->
        <activity
                android:name=".example.Camera2Activity"
                android:screenOrientation="portrait"/>
        <!-- 动画 -->
        <activity
                android:name=".other.AnimationAudioActivity"
                android:screenOrientation="portrait"/>
        <!-- 模拟快递布局 -->
        <activity
                android:name=".other.TraceListActivity"
                android:screenOrientation="portrait"/>
        <!-- 不同格式文件浏览 activity -->
        <activity
                android:name=".tbs.FileBrowsingActivity"
                android:screenOrientation="portrait"/>

        <!-- WebView 相关 activity -->
        <!-- 8.0关于WebView的新特性 -->

        <activity
                android:name=".web.WebViewActivity"
                android:screenOrientation="portrait">
            <meta-data
                    android:name="android.webkit.WebView.EnableSafeBrowing"
                    android:value="true">
            </meta-data>
        </activity>
        <!-- WebView -->
        <activity
                android:name=".web.StoreWebViewActivity"
                android:screenOrientation="portrait"/>
        <!--Animation -->
        <activity
                android:name=".animation.AnimationActivity"
                android:screenOrientation="portrait"/>
        <!-- RecyclerviewActivity -->
        <activity
                android:name=".channelmanage.ChannelManageActivity"
                android:screenOrientation="portrait"/>
        <!-- 权限相关 -->
        <activity
                android:name="com.fy.baselibrary.utils.PermissionActivity"
                android:screenOrientation="portrait"
                android:theme="@style/NoTitleTranslucentTheme"/>
        <!-- SurfaceView+MediaPlayer 自定义播放器 -->
        <activity
                android:name=".other.SurfaceActivity"
                android:configChanges="keyboardHidden|orientation|screenSize"
                android:screenOrientation="sensor"
                android:windowSoftInputMode="stateHidden|adjustResize"/>

    </application>

